import "lib/std/ops.tao"

class Zero =
    => zero : Self

member Nat of Zero =
    => zero = 0

member Int of Zero =
    => zero = 0

member Real of Zero =
    => zero = 0.0

class One =
    => one : Self

member Nat of One =
    => one = 1

member Int of One =
    => one = 1

member Real of One =
    => one = 1.0

class Num < Zero + One + Add + Mul
